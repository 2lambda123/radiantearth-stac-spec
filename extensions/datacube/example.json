{
  "stac_version": "0.7.0",
  "id": "datacube",
  "description": "Multi-dimensional data cube.",
  "links": [
    {
      "rel": "self",
      "href": "catalog.json"
    }
  ],
  "properties": {
    "cube:dimensions": {
      "x": {
        "type": "spatial",
        "axis": "x",
        "extent": [
          -180,
          180
        ],
        "reference_system": 4326
      },
      "y": {
        "type": "spatial",
        "axis": "y",
        "extent": [
          -90,
          90
        ],
        "reference_system": 4326
      },
      "pressure_levels": {
        "type": "spatial",
        "axis": "z",
        "extent": [
          0,
          1000
        ],
        "step": 100,
        "unit": "urn:ogc:def:uom:SI:Pa"
      },
      "metered_levels": {
        "type": "spatial",
        "axis": "z",
        "values": [
          0,
          10,
          25,
          50,
          100,
          1000
        ],
        "unit": "urn:ogc:def:uom:SI:metre"
      },
      "time": {
        "type": "temporal",
        "extent": [
          "2015-01-01T00:00:00Z",
          "2018-12-31T23:59:59Z"
        ],
        "step": "P1D"
      },
      "spectral": {
        "type": "bands",
        "values": [
          "red",
          "green",
          "blue"
        ]
      }
    }
  }
}